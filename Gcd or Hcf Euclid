import java.util.*;
public class GcdEuclid{
    public static void main(String[] args){
        Scanner s=new Scanner(System.in);
        int m=s.nextInt();
        int n=s.nextInt();
        while(m!=n){
            if(m>n){
                m=m-n;
            }
            n=n-m;
        }
        System.out.print(m);
    }
    
}
