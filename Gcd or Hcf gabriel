import java.util.*;
public class GcdOrHcfGabriel{
    public static void main(String [] args){
        Scanner s=new Scanner(System.in);
        int m=s.nextInt();
        int n=s.nextInt();
       while(n>0&&m>0){
           if(m>n){
               m=m%n;
           }
           else{
               n=n%m;
           }
       }
       System.out.print(Math.max(m,n));
    }
}
