import java.util.*;
public class UniqueChar{
    public static void main(String args[]){
        Scanner s=new Scanner(System.in);
        String st=s.next();
        List<Character> ch=new ArrayList<>();
        for(int i=0;i<st.length();i++){
            if(!ch.contains(st.charAt(i))){
                ch.add(st.charAt(i));
            }
        }
        
    System.out.print(ch.size());
}
}
